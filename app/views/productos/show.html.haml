- title "Producto"

%p
  %strong Proveedor:
  = @producto.proveedor_id
%p
  %strong Referencia:
  = @producto.referencia
%p
  %strong Caracteristica:
  = @producto.caracteristica

%p
  = link_to "Edit", edit_producto_path(@producto)
  |
  = link_to "Destroy", @producto, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", productos_path
