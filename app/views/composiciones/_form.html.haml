= form_for @composicion do |f|
  = f.error_messages
  %p
    = f.label :prueba_id
    %br
    = f.text_field :prueba_id
  %p
    = f.label :producto_id
    %br
    = f.text_field :producto_id
  %p
    = f.label :porcentaje
    %br
    = f.text_field :porcentaje
  %p
    = f.label :comentario
    %br
    = f.text_field :comentario
  %p
    = f.submit
